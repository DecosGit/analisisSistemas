        crossorigin="anonymous"></script>

</head>

<body>
    <div class="container">
        <div class="login-card">
            <h2>Crear Usuario</h2>
            <form action="" method="post">
                <label for="names">Nombre:</label>
                <input type="text" id="names" placeholder="Nombre" name="names" required>

                <label for="apellido">Apellido:</label>
                <input type="text" id="apellido" placeholder="Apellidos" name="apellido" required>

                <label for="dpi">Documento de Identificacion Personal DPI:</label>
                <input type="number" id="dpi" placeholder="xxxx-xxxxx-xxxx" name="dpi" required>

                <label for="phone">Telefono:</label>
                <input type="number" id="phone" placeholder="xxxx-xxxx" name="phone" required>
                
                <button type="submit">Iniciar Sesi√≥n</button>
            </form>
        </div>
    </div>
</body>

</html>